        <div class="form-group">
            <label>SINIF ADI</label>
            <input class="form-control" type="text" name="sinif" id="sinif" placeholder="SINIF ADI" required="required">
        </div>
        <div class="form-group">
            <label>SERVİS BEDELİ</label>
            <input class="form-control" type="text" name="servisBedeli" id="servisBedeli" placeholder="SERVİS BEDELİ" required="required">
        </div>



        <div class="box-footer">
            <button class="btn btn-xs btn-danger" id="btnSubmit" href="@Url.Action("SinifEkle", "Panel" )">
