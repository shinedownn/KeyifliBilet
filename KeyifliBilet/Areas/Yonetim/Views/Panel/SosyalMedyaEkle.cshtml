<form id="myForm" action="@Url.Action("SosyalMedyaEkle", "Panel")" method="post" accept-charset="utf-8" enctype="multipart/form-data">
    <div id="listOfCustomers" style="height: 400px; overflow: auto;">


        <div class="form-group">
            <label>PLATFORM</label>
            <input class="form-control" type="text" name="platform" id="platform" placeholder="PLATFORM" required="required">
        </div>
        <div class="form-group">
            <label>ADRES</label>
            <input class="form-control" type="text" name="url" id="url" placeholder="ADRES" required="required">
        </div>



        <div class="box-footer">
            <button class="btn btn-xs btn-danger" id="btnSubmit" href="@Url.Action("SosyalMedyaEkle", "Panel" )">
                <span> EKLE</span>
            </button>
        </div>


    </div>


</form>
