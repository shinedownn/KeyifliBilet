@model KeyifliBilet.Areas.Yonetim.Models.IndexViewModel


@foreach (var item in Model.kutuKategori)
{
    <form id="myForm" action="@Url.Action("KutuKategoriGuncelle", "Panel",new { id = item.KategoriId })" method="post" accept-charset="utf-8" enctype="multipart/form-data">
        <div id="listOfCustomers" style="height: 400px; overflow: auto;">


            <div class="form-group">
                <label>KUTU KATEGORİ</label>
                <input class="form-control" type="text" name="kutuKategori" id="kutuKategori" value="@item.Kategori" required="required">
            </div>

            <div class="box-footer">
                <button class="btn btn-xs btn-danger" id="btnSubmit" href="@Url.Action("KutuKategoriGuncelle", "Panel",new { id = item.KategoriId })">
                    <span> DÜZENLE</span>
                </button>
            </div>


        </div>


    </form>
}
